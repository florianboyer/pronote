####### Compile

application.o: ../../application.cpp ../../application.h \
		../../section.h \
		../../evaluation.h \
		../../matiere.h \
		../../note.h \
		../../etudiant.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o application.o ../../application.cpp

bulletin.o: ../../bulletin.cpp ../../bulletin.h \
		../../etudiant.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o bulletin.o ../../bulletin.cpp

etudiant.o: ../../etudiant.cpp ../../etudiant.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o etudiant.o ../../etudiant.cpp

evaluation.o: ../../evaluation.cpp ../../section.h \
		../../evaluation.h \
		../../matiere.h \
		../../note.h \
		../../etudiant.h \
		../../application.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o evaluation.o ../../evaluation.cpp

main.o: ../../main.cpp ../../application.h \
		../../section.h \
		../../evaluation.h \
		../../matiere.h \
		../../note.h \
		../../etudiant.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o main.o ../../main.cpp

matiere.o: ../../matiere.cpp ../../matiere.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o matiere.o ../../matiere.cpp

note.o: ../../note.cpp ../../evaluation.h \
		../../section.h \
		../../etudiant.h \
		../../matiere.h \
		../../application.h \
		../../note.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o note.o ../../note.cpp

section.o: ../../section.cpp ../../section.h \
		../../evaluation.h \
		../../matiere.h \
		../../note.h \
		../../etudiant.h \
		../../application.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o section.o ../../section.cpp

